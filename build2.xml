<target name="openaf">
    <exec executable="wget">
        <arg value="https://openaf.io/openaf.jar.repacked"/>
        <arg value="-O"/>
        <arg value="openaf.jar"/>
    </exec>
    <exec executable="wget">
        <arg value="https://openaf.io/openaf.jar"/>
        <arg value="-O"/>
        <arg value="openaf.jar.orig"/>
    </exec>
    <exec executable="java">
        <arg value="-jar"/>
        <arg value="openaf.jar"/>
        <arg value="--install"/>
    </exec>
    <exec executable="java">
	<arg value="-jar"/>
        <arg value="openaf.jar"/>
        <arg value="--opack"/>
        <arg value="-e"/>
	<arg value="install https://openaf.io/ocle/OpenCli.opack -cred wedo:Password1"/>
    </exec>
</target>
